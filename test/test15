{
   var a, b, c, d, e, result, i: int;
   a = 5;
   b = 10;
   c = 0;
   d = 1;
   e = 3;
   result = 0;

   while (a > 0) {
      if (a % 2 == 0) {
         result += b;
         a /= 2;
      } else {
         result += c;
         a -= 1;
      }
   }

   for (i = 0; i < e; i += 1) {
      result *= d;
   }

   print result;
}

